# This is an example file. Copy this to .env and fill in your actual values.
# Do not commit the .env file to version control.

# Set this to FALSE to use Google AI Studio's API key.
# Set this to TRUE to use Google Cloud Vertex AI.
GOOGLE_GENAI_USE_VERTEXAI=FALSE

# Your Google AI Studio API key.
# Required if GOOGLE_GENAI_USE_VERTEXAI is FALSE.
GOOGLE_API_KEY=PASTE_YOUR_ACTUAL_API_KEY_HERE

# Your Google Cloud Project ID.
# Required if GOOGLE_GENAI_USE_VERTEXAI is TRUE.
# GOOGLE_CLOUD_PROJECT=PASTE_YOUR_ACTUAL_PROJECT_ID

# The location of your Google Cloud project.
# Required if GOOGLE_GENAI_USE_VERTEXAI is TRUE.
# GOOGLE_CLOUD_LOCATION=us-central1

# Your Google Chat webhook URL for sending notifications.
# To get a webhook URL, go to your Google Chat space, click on the space name,
# then "Apps & integrations", then "Manage webhooks".
GOOGLE_CHAT_WEBHOOK_URL=PASTE_YOUR_WEBHOOK_URL_HERE
